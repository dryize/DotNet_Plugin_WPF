<UserControl x:Class="PluginDLL2.CalControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="126" d:DesignWidth="230" Background="#FF5DD827">
    <Grid Width="231">
        <Label Content="N 1 :" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="N 2 :" Height="28" HorizontalAlignment="Left" Margin="12,36,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="53,12,0,0" Name="textBox1" VerticalAlignment="Top" Width="84" DataContext="{Binding}" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="53,39,0,0" Name="textBox2" VerticalAlignment="Top" Width="84" DataContext="{Binding StringFormat=\{0:F0\}}" />
        <Button Content="Add" Height="50" HorizontalAlignment="Left" Margin="143,12,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Label Content="Sum Is :" Height="28" HorizontalAlignment="Left" Margin="33,86,0,0" Name="label3" VerticalAlignment="Top" Width="207" />
    </Grid>
</UserControl>
